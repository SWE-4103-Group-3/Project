<link href="/css/courselist.css" rel="stylesheet"/>

{% block courselist %}
<div class="list-group courselist">
    <p class="list-group-item active">Course List</p>
    {% for courseItem in courseList %}
        <a href="#" class="list-group-item courselist-item">{{ courseItem.getName() }}</a>
    {% endfor %}
</div>
{% endblock %}
